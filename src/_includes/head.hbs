<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta http-equiv="Content-Type" content="text/html; charset=utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1">

<title>{{htmlWebpackPlugin.options.site.title}}</title>
<meta name="description" content="{{htmlWebpackPlugin.options.site.description}}">

<link rel="canonical" href={{[canonicalGenerator] htmlWebpackPlugin.options.pageName}}>

<link rel="icon" href="./img/favicon.ico">

<meta property="fb:app_id" content="{{htmlWebpackPlugin.options.social.fb_app_id}}" />
<meta property="og:site_name" content="{{htmlWebpackPlugin.options.site.site_name}}">
<meta property="og:title" content="{{htmlWebpackPlugin.options.site.title}}">
<meta property="og:description" content="{{htmlWebpackPlugin.options.site.description}}">
<meta property="og:type" content="website">

{{#if htmlWebpackPlugin.options.site.facebook_image_url}}
  <meta property="og:image" content="{{htmlWebpackPlugin.options.social.facebook_image_url}}">
  <meta property="og:image:type" content="image/jpeg">
  <meta property="og:image:width" content="1200">
  <meta property="og:image:height" content="630">
{{/if}}

<meta property="og:url" content="">

<meta name="twitter:card" content="{{htmlWebpackPlugin.options.social.twitter_card}}">
<meta name="twitter:site" content="{{htmlWebpackPlugin.options.social.twitter-site}}">
<meta name="twitter:creator" content="{{htmlWebpackPlugin.options.social.twitter-creator}}">
<meta name="twitter:title" content="{{htmlWebpackPlugin.options.site.title}}">
<meta name="twitter:description" content="{{htmlWebpackPlugin.options.site.description}}">
{{#if htmlWebpackPlugin.options.site.twitter_image_url}}
  <meta name="twitter:image" content="{{htmlWebpackPlugin.options.social.twitter_image_url}}">
{{/if}}

<link href='https://fonts.googleapis.com/css?family=Open+Sans:400,300,700,300italic,400italic,700italic' rel='stylesheet' type='text/css'>

{{> ./tracking/segment}}
